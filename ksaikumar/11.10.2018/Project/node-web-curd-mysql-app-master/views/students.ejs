<div>
    <table border="1" cellpadding="7" cellspacing="7">
        <tr>
            <th width="50px">No</th>
            <th>id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            
        </tr>
        <% if(data.length){ 
                                
            for(var i = 0;i < data.length;i++) { %>
        <tr>
            <td><%=(i+1)%></td>
            <td><%=data[i].id%></td>
            <td><%=data[i].name%></td>
            <td><%=data[i].email%></td>
            <td><%=data[i].phone%></td>
        </tr>

        <% } }%>
    </table>
    </div>